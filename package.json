{
  "name": "generative-db",
  "version": "0.2.0",
  "type": "module",
  "description": "Instantly generate and populate a real database for a fictional company",
  "keywords": [
    "database",
    "generator",
    "sample",
    "faker",
    "chatgpt",
    "ai",
    "mock",
    "testing",
    "schema",
    "synthesized",
    "postgres",
    "mysql",
    "sqlite"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "author": "CNimmo16",
  "license": "MIT",
  "bin": {
    "generate": "dist/cli/generate.js",
    "apply": "dist/cli/apply.js"
  },
  "scripts": {
    "format": "prettier --write .",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "generate": "dotenvx run -f .env.local -- node dist/cli/generate.js",
    "apply": "dotenvx run -f .env.local -- node dist/cli/apply.js",
    "changeset": "tsc && changeset",
    "ci:release": "turbo run build lint test && changeset version && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.10",
    "@dotenvx/dotenvx": "^1.29.0",
    "@types/minimist": "^1.2.5",
    "prettier": "3.4.2"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.2.0",
    "@types/pg": "^8.11.10",
    "chalk": "^5.3.0",
    "graph-data-structure": "^4.3.0",
    "knex": "^3.1.0",
    "minimist": "^1.2.8",
    "openai": "^4.76.2",
    "pg": "^8.13.1",
    "typescript": "^5.7.2",
    "uuid": "^11.0.3",
    "yocto-spinner": "^0.1.2",
    "zod": "^3.24.1"
  }
}
